}
.coming-time{
	text-align: center;
	font-size: 16rpx;
	width: 80rpx;
	color: red;
	border: 1rpx solid red;
	border-radius: 10rpx;
}
.movies-list{
	margin-top: 10rpx;
	margin-bottom: 10px;
	background-color: #FFFFFF;
}
.movielist-swiper{
	height: 300rpx;
	margin:0 30rpx;
}
.movies-list .swiper-item{
	height: 300rpx;
}
.movielist-detail{
	background: #FFFFFF;
	padding: 30rpx 0;
}
.movie-container{
	height: 100%;
	width: 95%;
	margin: auto;
	background-color: blue;
	border-radius: 10rpx;
	position: relative;
	overflow: hidden;
}
.movies-ranking{
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	height: 150rpx;
	background: #3d464f;
	padding: 20rpx 30rpx 0;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.scroll-Y{
	height: 100%;
}
.movie-ranking-text{
	height: 30rpx;
	line-height: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
	color: #FFFFFF;
	font-size: 20rpx;
}
.movies-ranking-title{
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	height: 150rpx;
	background: rgba(0,0,0,0.5);
	line-height: 50rpx;
	text-align: left;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding-left: 30rpx;
	padding-top: 20rpx;
	color: #FFFFFF;
}
.movies-ranking-title view:first-child{
	font-size: 22rpx;
}
.movies-ranking-title view:last-child{
	font-size: 38rpx;
}
